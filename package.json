{
  "name": "animere",
  "version": "1.6.0",
  "description": "CSS-driven scroll-based animations",
  "type": "module",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && cp dist/animere.min.js docs/js/animere.min.js && npm run types",
    "types": "npx tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "size": "cat dist/animere.min.js | gzip | wc -c",
    "test": "echo \"No test specified\""
  },
  "dependencies": {
    "animate.css": "^4.1.1"
  },
  "devDependencies": {
    "rollup": "^2.40.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "files": [
    "dist/*",
    "types/*",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/animere.git"
  },
  "keywords": [
    "animation",
    "animate.css",
    "on-scroll",
    "scroll-animation",
    "scroll-story"
  ],
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johannschopplich/animere/issues"
  },
  "homepage": "https://github.com/johannschopplich/animere#readme"
}
